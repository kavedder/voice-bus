<?xml version= "1.0"?>
<grammar xml:lang="en-US" root ="LINKGRAM">
  <rule id="LINKGRAM" scope="public">
    <item repeat="0-1"> please </item>
    <one-of>
      <item>main</item>
      <item>back</item>
      <item>begin</item>
      <item>start over</item>
      <item>start again</item>
      <item>menu</item>
      <item>main menu</item>
    </one-of>
    <item repeat="0-1"> please </item>
  </rule>
</grammar>
